"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/Voxel.gd"
}, {
"base": "MeshInstance",
"class": "VoxelMesh",
"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/VoxelMesh.gd"
}, {
"base": "Reference",
"class": "VoxelObjectEditor",
"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/VoxelObjectEditor.gd"
}, {
"base": "Reference",
"class": "VoxelObjectEditorTool",
"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/VoxelObjectEditorTool.gd"
}, {
"base": "Resource",
"class": "VoxelSet",
"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/VoxelSet.gd"
}, {
"base": "SurfaceTool",
"class": "VoxelTool",
"language": "GDScript",
"path": "res://addons/Voxel-Core/classes/VoxelTool.gd"
} ]
_global_script_class_icons={
"Voxel": "res://addons/Voxel-Core/assets/classes/Voxel.png",
"VoxelMesh": "res://addons/Voxel-Core/assets/classes/VoxelMesh.png",
"VoxelObjectEditor": "res://addons/Voxel-Core/assets/classes/VoxelEditor.png",
"VoxelObjectEditorTool": "res://addons/Voxel-Core/assets/classes/VoxelEditorTool.png",
"VoxelSet": "res://addons/Voxel-Core/assets/classes/VoxelSet.png",
"VoxelTool": "res://addons/Voxel-Core/assets/classes/VoxelTool.png"
}

[application]

config/name="Voxel-Core"
boot_splash/image="res://assets/VoxelCore.splash.png"
boot_splash/bg_color=Color( 1, 1, 1, 1 )
config/icon="res://icon.png"

[editor_plugins]

enabled=PoolStringArray( "Voxel-Core" )

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
